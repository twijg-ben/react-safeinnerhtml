  "pre-commit": [
    "lint-staged"
  ],
  "directories": {
    "lib": "./dist"
  },
  "peerDependencies": {
    "classnames": ">=2.2.0",
    "lodash": ">=4.0.0",
    "react": ">=15.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-env": "1.2.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "classnames": ">=2.2.0",
    "cross-env": "3.2.3",
    "lint-staged": "3.3.1",
    "lodash": ">=4.0.0",
    "pre-commit": "1.2.2",
    "prettier": "0.21.0",
    "react": ">=15.0.0"
  },
  "homepage": "https://github.com/twijg/react-safeinnerhtml",
  "repository": {
    "url": "https://github.com/twijg/react-safeinnerhtml",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/twijg/react-safeinnerhtml/issues"
  }
