{
  "name": "react-safeinnerhtml",
  "version": "0.1.0",
  "description": "React Component to safely use HTML in the DOM",
  "keywords": [
    "react",
    "react-component",
    "html",
    "react-dom",
    "dom",
    "javascript"
  ],
  "author": "Twijg IT B.V. <info@twijg-it.nl>",
  "contributors": [
    "Wiebe Cnossen <wiebe@twijg-it.nl>",
    "Jelle de Jong <jelle@twijg-it.nl>"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js*",
    "prettier": "prettier --write src/**/*.js*",
    "build": "cross-env NODE_ENV=production babel src/ --out-dir dist/",
    "lint-staged": "lint-staged",
    "lint-staged:eslint": "eslint -c .eslintrc",
    "lint-staged:prettier": "prettier --write"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "lint-staged:prettier",
      "lint-staged:eslint",
      "git add"
    ]
  },
  "pre-commit": ["lint-staged"],
  "dependencies": {
    "classnames": "^2.2",
    "lodash": "^4.0.0",
    "prop-types": "^15.5.8",
    "react": "^16"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5",
    "eslint": "^4",
    "eslint-plugin-react": "^7",
    "lint-staged": "^5",
    "pre-commit": "^1",
    "prettier": "^1"
  },
  "homepage": "https://github.com/twijg/react-safeinnerhtml",
  "repository": {
    "url": "https://github.com/twijg/react-safeinnerhtml",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/twijg/react-safeinnerhtml/issues"
  }
}
