{
  "name": "react-safeinnerhtml",
  "version": "0.0.5",
  "description": "React Component to safely use HTML in the DOM",
  "keywords": [
    "react",
    "react-component",
    "html",
    "react-dom",
    "dom",
    "javascript"
  ],
  "author": "Twijg IT B.V. <info@twijg-it.nl>",
  "contributors": [
    "Wiebe Cnossen <wiebe@twijg-it.nl>",
    "Jelle de Jong <jelle@twijg-it.nl>"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js*",
    "prettier": "prettier --write src/**/*.js*",
    "test": "",
    "build": "cross-env NODE_ENV=production babel src/ --out-dir dist/",
    "lint-staged": "lint-staged",
    "lint-staged:eslint": "eslint -c .eslintrc",
    "lint-staged:prettier": "prettier --write"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "lint-staged:prettier",
      "lint-staged:eslint",
      "git add"
    ]
